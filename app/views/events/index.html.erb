<%= provide(:title, 'all events') %>
<%= render 'shared/navbar_dark' %>

<p id="notice"><%= notice %></p>

<h1>Events</h1>
<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.name %></td>
        <td><%= event.location %></td>
        <td><%= event.latitude %></td>
        <td><%= event.longitude%></td>
<!--        <td><%#= button_to 'Sign up', signup_for_event_path, method: :post, params: {this_event: event}, class: "btn btn-sm btn-block"%><td>-->
        <td><p> event page </p></td>
        <td><p> map </p></td>
       
      </tr>
    <% end %>
  </tbody>
</table>

<br>


