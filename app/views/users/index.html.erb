<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>My Events</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% current_user.events.each do |event| %>
      <tr>
        <td><%=event.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


